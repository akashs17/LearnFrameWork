<?xml version="1.0" encoding="UTF-8"?>

<suite name="Demo Suite" verbose="1" parallel="tests" thread-count="3" >

 <listeners>
      <listener class-name="org.uncommons.reportng.HTMLReporter"/>
      <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
      <listener class-name="exzeo.lib.CustomisedListener"></listener>
  </listeners>

 	<test name="LoginFunctonality for Valid User Name and Password">
		<classes>
			<class
				name="exzeo.TestCases.ValidateLoginFunctionalityForValidUserNameAndPassword" />
		</classes>
	</test>

	<test name="LoginFunctonality for blank User Name and Password">
		<classes>
			<class
				name="exzeo.TestCases.ValidationLoginFunctionalityForBlankUserNameAndPassword" />
		</classes>
	</test>


	
</suite>
